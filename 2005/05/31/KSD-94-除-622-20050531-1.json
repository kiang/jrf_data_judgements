           │天祥分社  │            │                │              │            │      │\n├──┼──────┼─────┼──────┼────────┼───────┼──────┼───┤\n│13  │黃福龍      │華南商業銀│000000000   │68,000元        │93年9月30日   │0000000     │      │\n│    │            │行路竹分行│            │                │              │            │      │\n├──┼──────┼─────┼──────┼────────┼───────┼──────┼───┤\n│14  │孫楊美香    │高新商業銀│19316       │15,800元        │93年9月30日   │0000000     │      │\n│    │            │行大順分行│            │                │              │            │      │\n├──┼──────┼─────┼──────┼────────┼───────┼──────┼───┤\n│15  │陳秀英      │高雄銀行右│169450      │30,000元        │93年9月30日   │81134       │      │\n│    │            │昌分行    │            │                │              │            │      │\n├──┼──────┼─────┼──────┼────────┼───────┼──────┼───┤\n│16  │舜紘企業有限│合作金庫銀│000000000   │19,068元        │93年9月30日   │0000000     │      │\n│    │公司        │行前鎮分行│            │                │              │            │      │\n├──┼──────┼─────┼──────┼────────┼───────┼──────┼───┤\n│17  │台灣科邑科技│台灣銀行前│54277       │156,640元       │93年9月30日   │0000000     │      │\n│    │股份有限公司│鎮分行    │            │                │              │            │      │\n├──┼──────┼─────┼──────┼────────┼───────┼──────┼───┤\n│18  │潘璞        │國泰世華銀│9188        │2,010元         │93年9月30日   │441531      │      │\n│    │            │行鳳山分行│            │                │              │            │      │\n├──┼──────┼─────┼──────┼────────┼───────┼──────┼───┤\n│19  │周黃春金    │合作金庫銀│220881      │38,136元        │93年9月30日   │0000000     │      │\n│    │            │行憲德分行│            │                │              │            │      │\n├──┼──────┼─────┼──────┼────────┼───────┼──────┼───┤\n│20  │莊成安      │高雄市第三│16284       │6,000元         │93年9月30日   │304951      │      │\n│    │            │信用合作社│            │                │              │            │      │\n│    │            │青年分社  │            │                │              │      